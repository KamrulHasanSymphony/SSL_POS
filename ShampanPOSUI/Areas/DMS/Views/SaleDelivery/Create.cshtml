@model ShampanPOS.Models.SaleDeliveryVM
@{
    ViewBag.Title = "Sale Delivery Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    var serial = 1;
}
<style>
    .auditshow {
        height: 39px;
        width: 37px;
        padding: 6px;
        border: 1px solid #d5bdbd;
        border-radius: 50%;
        display: flex;
        justify-content: center;
        align-items: center;
        margin-right: 5px;
        background: green;
        box-shadow: rgba(0, 0, 0, 0.25) 0px 14px 28px, rgba(0, 0, 0, 0.22) 0px 10px 10px;
    }
</style>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"> Sale Delivery Entry </h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li id="btnBack"><img src="~/Content/css/ButtonsIcons/back-icon.png" title="Click for Go Back" /></li>
                </ol>
            </div>
        </div>
    </div>
</div>

<div class="card">
    <div class="card-header p-2">

        @if (Model.IsPost)
        {
            <button type="button" class="button sslPush">Already Posted</button>

            <button style="float:right;" type="button" title="Add New" data-url="/DMS/SaleDelivery/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                New
            </button>
        }
        else if (Model.Operation.Equals("update", StringComparison.OrdinalIgnoreCase))
        {
            <button type="button" class="button btnsave sslUpdate">Update</button>

            <button type="button" class="button btnPost sslPost">Post</button>

            <button type="button" style="display:none;" class="button sslPush">Already Posted</button>

            <button style="float:right;" type="button" title="Add New" data-url="/DMS/SaleDelivery/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                New
            </button>
        }
        else
        {
            <div style="display:none;" class="divUpdate">
                <button type="button" class="button btnsave sslUpdate">Update</button>
                <button type="button" class="button btnPost sslPost">Post</button>
                <button type="button" style="display:none;" class="button sslPush">Already Posted</button>

                <button style="float:right;" type="button" title="Add New" data-url="/DMS/SaleDelivery/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                    New
                </button>

            </div>
            <div class="divSave">
                <button class="button btnsave sslSave" type="button" id="">Save</button>
            </div>
        }

    </div>
</div>

<div class="container-fluid">
    <form id="frmEntry">
        <div class="card card-secondary">
            <div class="card-header">
                <h3 class="card-title"> Sale Delivery Info </h3>
            </div>
            <div class="card-body">

                @Html.HiddenFor(model => model.Id)
                @Html.HiddenFor(model => model.Operation)
                @Html.HiddenFor(model => model.DecimalPlace)
                @Html.HiddenFor(model => model.SaleOrderId)
                @Html.HiddenFor(model => model.SaleOrderDetailId)
                @Html.HiddenFor(model => model.UOMId)
                @Html.HiddenFor(model => model.IsPost)
                @Html.HiddenFor(model => model.BranchId)
                @Html.HiddenFor(model => model.TransactionType)
                @Html.HiddenFor(model => model.FromDate)
                @Html.HiddenFor(model => model.InvoiceDiscountRate)
                @Html.HiddenFor(model => model.InvoiceDiscountAmount)
                @Html.HiddenFor(model => model.GrandTotalVATAmount)
                @Html.HiddenFor(model => model.GrandTotalSDAmount)

                <div class="form-group row">

                    <div class="col-sm-2 codeWidth">
                        @Html.LabelFor(model => model.Code)
                        <div style="display: flex; align-items: center;">
                            <span id="btnPrevious" class="fa fa-backward" style="color: darkseagreen;"></span>&nbsp;
                            @Html.TextBoxFor(model => model.Code, new { placeholder = "Code Auto Generate", @class = " csCode  form-control form-control-sm", @readonly = "readonly", @style = "width:140px;" }) &nbsp;
                            <span id="btnNext" class="fa fa-forward" style="color: darkseagreen;"></span>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.SalePersonId)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.SalePersonId, new { @class = "form-control form-control-sm" })
                        </div>
                        <span id="titleError1" class="text-danger" style="display:none;"></span>
                    </div>
                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.CustomerId)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.CustomerId, new { @class = "form-control form-control-sm" })
                        </div>
                        <span id="titleError2" class="text-danger" style="display:none;"></span>
                    </div>
                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.DeliveryPersonId)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.DeliveryPersonId, new { @class = "form-control form-control-sm" })
                        </div>

                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.DriverPersonId)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.DriverPersonId, new { @class = "form-control form-control-sm" })
                        </div>
                        <span id="titleError5" class="text-danger" style="display:none;"></span>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.InvoiceDateTime)
                        <div class="input-group date">
                            @Html.TextBoxFor(model => model.InvoiceDateTime, new { @class = "  form-control form-control-sm dateRange" })
                            <div class="input-group-append">
                                <div class="input-group-text"><i class="far fa-calendar-alt"></i></div>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="form-group row">

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.DeliveryDate)
                        <div class="input-group date">
                            @Html.TextBoxFor(model => model.DeliveryDate, new { @class = "  form-control form-control-sm dateRange" })
                            <div class="input-group-append">
                                <div class="input-group-text"><i class="far fa-calendar-alt"></i></div>
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.CurrencyId)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.CurrencyId, new { @class = "form-control form-control-sm" })
                        </div>
                        <span id="titleError3" class="text-danger" style="display:none;"></span>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.VehicleNo)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.VehicleNo, new { placeholder = "Enter Vehicle Number", @class = "form-control form-control-sm" })
                        </div>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.VehicleType)
                        <div class="input-group">
                            @Html.TextBoxFor(model => model.VehicleType, new { placeholder = "Enter Vehicle Type", @class = "form-control form-control-sm" })
                        </div>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.DeliveryAddress)
                        <div class="input-group">
                            @Html.TextAreaFor(model => model.DeliveryAddress, new { placeholder = "Enter Delivery Address", @class = "form-control form-control-sm" })
                        </div>
                    </div>

                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.Comments)
                        <div class="input-group">
                            @Html.TextAreaFor(model => model.Comments, new { placeholder = "Enter Comments", @class = "form-control form-control-sm" })
                        </div>
                    </div>

                </div>

            </div>
        </div>

        <div class="card card-secondary">
            <div class="card-header">
                <h3 class="card-title"> Details </h3>
            </div>
            <div class="card-body">

                <button type="button" title="Add Details" id="addRows" class="btn btn-lg btn-success mb-2"><i class="fa fa-plus"></i></button>

                <div class="clearfix">&nbsp;</div>

                <div style="overflow-x: auto; white-space: nowrap;">
                    <table class="table table-bordered table-striped table-sm" id="details">
                        <thead>
                            <tr>
                                <th data-type="text" data-computed="true" data-required="false" style="width:5%;" data-name="SLNo">SL. No.</th>
                                <th hidden data-type="text" data-computed="true" data-required="false" style="width:20%;" data-name="ProductCode"> Product Code </th>
                                <th data-type="text" data-search="true" data-required="true" style="width:20%;" data-name="ProductName"> Product </th>

                                <th hidden data-type="text" data-search="false" style="width:5%;" data-name="ProductId"> ProductId </th>
                                <th data-type="text" data-search="true" data-computed="false" data-required="true" style="width:10%;" data-name="UOMName"> UOM</th>
                                <th hidden data-type="text" data-search="false" style="width:5%;" data-name="UOMId"> UOMId </th>
                                <th data-type="decimal" style="width: 10%;" data-required="true" data-name="CtnQuantity">Ctn Qty </th>
                                <th data-type="decimal" style="width: 10%;" data-required="true" data-name="InputQuantity"> Input Qty </th>

                                <th data-type="decimal" style="width: 10%;" data-required="true" data-computed="true" data-name="PcsQuantity"> Pcs Qty </th>
                                <th data-type="decimal" style="width: 10%;" data-required="true" data-name="Quantity"> Total Qty </th>

                                <th data-type="decimal" style="width: 10%;" data-required="true" data-name="UnitRate"> Unit Rate </th>

                                <th data-type="text" style="width:12%;" data-computed="true" data-name="FreeProductName">Free Product</th>
                                <th hidden data-type="text" data-search="false" style="width:5%;" data-name="FreeProductId"> FreeProductId </th>
                                <th data-type="decimal" style="width: 5%;" data-computed="true" data-name="FreeQuantity"> Free Quantity </th>
                                <th hidden data-type="decimal" style="width: 5%;" data-computed="true" data-name="DiscountRate"> Discount Rate </th>
                                <th data-type="decimal" style="width: 5%;" data-computed="true" data-name="DiscountAmount"> Discount Amount </th>

                                <th data-type="decimal" style="width: 14%;" data-required="true" data-computed="true" data-name="SubTotal"> SubTotal </th>
                                <th hidden data-type="decimal" style="width: 5%;" data-computed="true" data-name="SubTotalAfterDiscount"> SubTotal AfterDiscount </th>


                                <th hidden data-type="decimal" style="width: 8%;" data-computed="true" data-name="SD"> SD Rate </th>
                                <th hidden data-type="decimal" style="width: 8%;" data-computed="true" data-name="SDAmount"> SD Amount </th>
                                <th hidden data-type="decimal" style="width: 8%;" data-computed="true" data-name="VATRate"> VAT Rate </th>
                                <th hidden data-type="decimal" style="width: 8%;" data-computed="true" data-name="VATAmount"> VAT Amount </th>

                                <th data-type="decimal" style="width: 8%;" data-computed="true" data-name="LineTotal"> Line Total </th>
                                <th hidden data-type="decimal" style="width: 5%;" data-computed="true" data-name="LineDiscountRate"> Line DiscountRate </th>
                                <th hidden data-type="decimal" style="width: 5%;" data-computed="true" data-name="LineDiscountAmount"> Line DiscountAmount </th>
                                <th hidden data-type="decimal" style="width: 5%;" data-computed="true" data-name="LineTotalAfterDiscount"> LineTotal AfterDiscount </th>

                                <th data-type="text" style="width: 15%;" data-name="Comments">Comments</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="CampaignTypeId">CampaignTypeId</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="CampaignDetailsId">CampaignDetailsId</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="CampaignHeaderId">CampaignHeaderId</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="SaleOrderId">SaleOrderId</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="FromSaleOrderDetailId">FromSaleOrderDetailId</th>
                                <th hidden data-type="text" style="width: 5%;" data-name="UOMConversion">UOMConversion</th>

                                @*<th hidden data-type="text" style="width: 5%;" data-name="SaleOrderDetailId">SaleOrderDetailId</th>*@
                            </tr>
                        </thead>
                        <tbody id="lst">
                            @if (Model != null)
                            {
                                foreach (var item in Model.saleDeliveryDetailList)
                                {
                                    <tr>
                                        <td> @serial </td>
                                        <td hidden> @item.ProductCode </td>
                                        <td> @item.ProductName </td>
                                        <td hidden> @item.ProductId </td>

                                        <td> @item.UOMName </td>
                                        <td hidden>@item.UOMId</td>
                                        <td class=" dFormat "> @item.CtnQuantity </td>
                                        <td class=" dFormat "> @item.InputQuantity </td>

                                        <td class=" dFormat "> @item.PcsQuantity </td>
                                        <td class="dFormat "> @item.Quantity </td>

                                        <td class=" dFormat "> @item.UnitRate </td>
                                        <td> @item.FreeProductName </td>

                                        @*<td > @item.FreeProductName </td>*@
                                        <td hidden> @item.FreeProductId </td>
                                        <td class=" dFormat "> @item.FreeQuantity </td>
                                        <td hidden class=" dFormat "> @item.DiscountRate </td>
                                        <td class=" dFormat "> @item.DiscountAmount </td>
                                        <td class=" dFormat "> @item.SubTotal</td>
                                        <td hidden class=" dFormat ">@item.SubTotalAfterDiscount</td>
                                        <td hidden class=" dFormat "> @item.SD</td>
                                        <td hidden class=" dFormat "> @item.SDAmount</td>
                                        <td hidden class=" dFormat "> @item.VATRate</td>
                                        <td hidden class=" dFormat "> @item.VATAmount</td>

                                        <td class=" dFormat ">@item.LineTotal</td>
                                        <td hidden class=" dFormat ">@item.LineDiscountRate</td>
                                        <td hidden class=" dFormat ">@item.LineDiscountAmount</td>
                                        <td hidden class=" dFormat ">@item.LineTotalAfterDiscount</td>
                                        <td>@item.Comments</td>
                                        <td hidden> @item.SaleOrderId</td>
                                        <td hidden> @item.SaleOrderDetailId</td>
                                        <td hidden> @item.CampaignTypeId</td>
                                        <td hidden> @item.CampaignDetailsId</td>
                                        <td hidden> @item.CampaignHeaderId</td>
                                        <td hidden> @item.UOMConversion</td>

                                        <td> <button class="btn btn-danger btn-sm remove-row-btn"><i class='fa fa-trash'></i></button> </td>

                                    </tr>

                                    { serial++; }
                                }
                            }


                        </tbody>

                    </table>
                </div>

            </div>
        </div>

        <div class="card">
            <div class="card-header p-2">

                <div class="card-body">
                    <div class="row">


                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp;Grand Total Quantity </label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("GrandTotalAmount", 0, new { @placeholder = "Grand Total Amount", @class = " dFormat trGrandTotalAmount form-control form-control-sm   priceLen NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp;Total Free Quantity </label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("FreeGrandTotalAmount", 0, new { @placeholder = "Grand Total with Free Amount", @class = " dFormat trFreeGrandTotalAmount form-control form-control-sm   priceLen NumberCheck text-right", @readonly = "readonly" })
                            </div>

                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp;Grand SubTotal </label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("GrandSubTotal", 0, new { @placeholder = "Grand SubTotal", @class = " dFormat trGrandSubTotal form-control form-control-sm   priceLen NumberCheck text-right", @readonly = "readonly" })
                            </div>

                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Grand Line Total</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("GrandTotal", 0, new { @placeholder = "Grand Line Total", @class = "trGrandTotal form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-3">

                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Regular Discount Rate %</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBoxFor(model => model.RegularDiscountRate, new { @class = "trRegularDiscountRate form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })

                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Regular Discount Amount</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBoxFor(model => model.RegularDiscountAmount, new { @class = "trRegularDiscountAmount form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })

                            </div>

                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; </label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("AfterRegularDiscountAmount", 0, new { @placeholder = " After Regular Discount amount", @class = "tramountRegularDiscountAmount form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                    </div>

                    <div class="row" style="display:none">
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp;Invoice Discount Rate</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("InvoiceDiscountRate", 0, new { @placeholder = "Final Invoice Value", @class = "trFinalInvoiceDiscount form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-3"></div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Special Discount Rate %</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBoxFor(model => model.SpecialDiscountRate, new { @placeholder = "Special Discount Rate", @class = "trSpecialDiscountRate form-control form-control-sm   NumberCheck text-right" })

                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Special Discount Amount</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBoxFor(model => model.SpecialDiscountAmount, new { @placeholder = " Special Discount", @class = "trSpecialDiscountAmount form-control form-control-sm   NumberCheck text-right" })

                            </div>

                        </div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; </label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("AfterSpecialDiscountAmount", 0, new { @placeholder = "After Special Discount", @class = "trAfterSpecialDiscountAmount form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                    </div>


                    <div class="row">
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3"></div>
                        <div class="col-md-3">
                            <div class="editor-label">
                                <label>&nbsp; Final Invoice Value</label>
                            </div>
                            <div class="editor-field">
                                @Html.TextBox("FinalInvoiceValue", 0, new { @placeholder = "Final Invoice Value", @class = "trFinalInvoiceValue form-control form-control-sm   NumberCheck text-right", @readonly = "readonly" })
                            </div>
                        </div>

                    </div>

                </div>

            </div>
        </div>
     

        <div class="card" style="flex-direction: row; justify-content: space-between; align-items: center;">
            <div class="card-header p-2">

                @if (Model.IsPost)
                {

                    <button style="float:left; margin-right: 5px;" type="button" class="button btnsave sslUpdate">Update</button>
                    <button style="float: right; margin-right: 5px;" type="button" title="Add New" data-url="/DMS/SaleDelivery/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                        New
                    </button>

                    <button type="button" class="button sslPush">Already Posted</button>
                }
                else if (Model.Operation.Equals("update", StringComparison.OrdinalIgnoreCase))
                {
                    @*<button style="float:left;" type="button" class="button btnsave sslUpdate">Update</button>
                    <button style="float: right; margin-left: 4px;" type="button" title="Add New" data-url="/DMS/SaleOrder/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                        New
                    </button>*@

                    <div style="display: flex; align-items: center; gap: 5px;">
                        <button style="float:left;" type="button" class="button btnsave sslUpdate">Update</button>
                        <button type="button" class="button btnPost sslPost">Post</button>
                        <button class="button sslPrintC " data-id="@Model.Id" style="float:right;">Print</button>
                        <button type="button" style="display:none;" class="button sslPush">Already Posted</button>
                    </div>


                }
                else
                {
                    @*<div style="display:none;" class="divUpdate">
                        <button type="button" class="button btnsave sslUpdate">Update</button>
                        <button style="float: right; margin-left: 2px;" type="button" title="Add New" data-url="/DMS/SaleOrder/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                            New
                        </button>
                    </div>
                    <div class="divSave">
                        <button class="button btnsave sslSave" type="button" id="">Save</button>
                    </div>*@


                    <div style="display:none;" class="divUpdate">
                        <button type="button" class="button btnsave sslUpdate">Update</button>
                        @*<button style="float: right; margin-left: 2px;" type="button" title="Add New" data-url="/DMS/SaleOrder/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                            New
                        </button>*@

                        <button type="button" class="button btnPost sslPost">Post</button>
                        <button type="button" style="display:none;" class="button sslPush">Already Posted</button>
                    </div>
                    <div class="divSave">
                        <button class="button btnsave sslSave" type="button" id="">Save</button>
                    </div>

                }

            </div>
            <div class="auditshow"> <img src="~/Content/Images/audit.png" title="Click for Audit" /> </div>
        </div>
    </form>

    
    <div class="card card-secondary auditcard">

        <div class="card-header">
            <h3 class="card-title">Audit Details</h3>
        </div>

        <div class="card-body">
            <div class="row">
                <div class="col-sm-2">
                    <div class="row">
                        @Html.Label("Type")
                    </div>
                </div>
                <div class="col-sm-10">
                    <div class="row">
                        <div class="col-sm-3">
                            @Html.Label("Created")
                        </div>
                        <div class="col-sm-3">
                            @Html.Label("Updated")
                        </div>
                        <div class="col-sm-3">
                            @Html.Label("Posted")
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-2">
                    <div class="row">
                        <div class="col-sm-12">
                            @Html.Label("By")<br /><div class="clearfix">&nbsp;</div>
                            @Html.Label("On")<br /><div class="clearfix">&nbsp;</div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-10">
                    <div class="row">
                        <div class="col-sm-3">
                            @Html.TextBoxFor(model => model.CreatedBy, new { @class = "CreatedBy  form-control form-control-sm ", @readonly = "readonly" })
                            <div class="clearfix">&nbsp;</div>
                            @Html.TextBoxFor(model => model.CreatedOn, new { @class = "CreatedOn  form-control form-control-sm ", @readonly = "readonly" })
                        </div>

