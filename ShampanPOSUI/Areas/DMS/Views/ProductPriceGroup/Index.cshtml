@model ShampanPOS.Models.ProductPriceGroupVM
@{
    ViewBag.Title = "Product Price Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"> Product Price Group List </h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    
                </ol>
            </div>
        </div>
    </div>
</div>


<div class="container-fluid">
    <div class="card">
        <div class="card-header p-2">
            <button type="button" title="Add New" data-url="/DMS/ProductPriceGroup/Create" onclick="GoTo(this)" class="button NewButton addNewButton">
                New
            </button>           
        </div>
    </div>
</div>


<div class="container-fluid">
    <div class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title">
                List Of Data
            </h3>
        </div>
        <div class="card-body">
            <div id="ProductPriceGroupGrid"></div>
        </div>
    </div>
</div>


@section scripts{

    <script src="~/Areas/Common/js/Services/CommonAjaxService.js"></script>
    <script src="~/Areas/DMS/js/Controllers/ProductPriceGroupController.js"></script>

    <script>
        $(document).ready(function () {
            ProductPriceGroupController.init();
        });

    </script>


}


